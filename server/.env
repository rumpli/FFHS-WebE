PORT=8080
NODE_ENV=production
DATABASE_URL="postgresql://tower:towerpw@db:5432/towerlords?schema=public"
REDIS_URL=redis://redis:6379
JWT_SECRET=dev-secret-change
OTEL_SERVICE_NAME=towerlords-backend
OTEL_TRACES_SAMPLER=parentbased_always_on
OTEL_RESOURCE_ATTRIBUTES=deployment.environment=dev
OTEL_EXPORTER_OTLP_TRACES_ENDPOINT=http://otel-collector:4318/v1/traces
OTEL_EXPORTER_OTLP_LOGS_ENDPOINT=http://otel-collector:4318/v1/logs
OTEL_EXPORTER_OTLP_METRICS_ENDPOINT=http://otel-collector:4318/v1/metrics
FASTIFY_LOG_LEVEL=warn

# Debug logs
LOG_DEBUG=0

# Allow end of round from client
ALLOW_CLIENT_END_ROUND=0